{
    "name": "pdombrovsky/dyna-exp",
    "type": "library",
    "description": "Fluent builders and typed helpers for DynamoDB expressions",
    "keywords": [
        "dynamodb",
        "aws",
        "expression",
        "builder",
        "query",
        "update"
    ],
    "homepage": "https://github.com/pdombrovsky/dyna-exp",
    "license": "MIT",
    "authors": [
        {
            "name": "pdombrovsky",
            "email": "dambrouskip@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DynaExp\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "DynaExp\\": "src/",
            "DynaExp\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "phpstan/phpstan": "^1.12"
    }
}
